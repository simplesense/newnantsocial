<tr>
  <td><%= pin.description %></td>
  <td><%= link_to time_ago_in_words(pin.created_at) + " ago", pin %></td>
  <% if current_user == pin.user %>
  <td><%= link_to 'Edit', edit_pin_path(pin) %></td>
  <td><%= link_to 'Delete', pin, method: :delete, data: { confirm: 'Are you sure you want to delete this?' } %></td>
  <% end %>
</tr>